//引用不能再赋值
#include<iostream>
using namespace std;

void main()
{
	double test1 = 100;
	double &later1 = test1;
	cout << "test1 = " << test1 << " later1 = " << later1 << endl;
	cout << "test1 = " << &test1 << " later1 = " << &later1 << endl;
	double test2 = 200;
	later1 = test2;
	cout << "test1 = " << test1 << " later1 = " << later1 << " test2 = " << test2 << endl;
	cout << "test1 = " << &test1 << " later1 = " << &later1 << " test2 = " << &test2 << endl;
}
