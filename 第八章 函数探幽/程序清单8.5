//引用的属性及其特别之处
#include<iostream>

using namespace std;

double cube(double);
double recube(double&);

void main()
{
	double n = 10;
	cout << cube(n) << "\t" << n << endl;
	cout << recube(n) << "\t" << n << endl;
}

double cube(double n)
{
	return n *= n*n;
}

double recube(double &n)
{
	return n *= n*n;
}
